#include <bits/stdc++.h>
using namespace std;

#define IOS                  \
    ios::sync_with_stdio(0); \
    cin.tie(0);              \
    cout.tie(0);
#define ll long long

void solve(){
    ll n,m;cin>>n>>m;
    ll s1 = 0, s2 = 0;
    for(ll i=0; i<n; i++){
        ll x;cin>>x;
        s1 += x;
    }
    for (ll i = 0; i < m; i++){
        ll x;cin>>x;
        s2 += x;
    }
    if(s1>s2){
        cout<<"Tsondu\n";
    }
    else if(s2>s1){
        cout<<"Tenzing\n";
    }
    else cout<<"Draw\n";
}

int32_t main(){
    IOS;
    ll t;
    cin>>t;
    while(t--){
    solve();
    }
    return 0;
}
