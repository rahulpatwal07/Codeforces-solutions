#include<bits/stdc++.h>
using namespace std;

void solve(){
    int n;cin>>n;
    unordered_map<string,int> mp;
    for (int i = 0; i < n; i++){
        string s;cin>>s;
        mp[s]++;
    }
    int maxi = 1;
    for(auto it : mp){
        maxi = max(maxi,it.second);
    }
    cout<<maxi<<endl;
    
    
}
int main(){
    // int t;cin>>t;
    // while(t--){
        solve();
    // }
	return 0;
}
