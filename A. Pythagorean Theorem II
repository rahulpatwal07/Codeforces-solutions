#include<bits/stdc++.h>
using namespace std;
void solve(){
    int n,k;cin>>n;
    int cnt = 0;
    for (int i = 0; i < n; i++){    
        for (int j = i+1; j < n; j++){
            int sum = i*i + j*j;
            int k = sqrt(sum);
            if(k*k==sum && i+j>k && k<=n){
                cnt++;
            }
        }
        
    }
    cout<<cnt<<endl;
}
int main(){
    // int t;cin>>t;
    // while(t--){
        solve();
    // }
	return 0;
}
