#include <bits/stdc++.h>
using namespace std;
 
#define IOS                  \
    ios::sync_with_stdio(0); \
    cin.tie(0);              \
    cout.tie(0);
#define endl "\n"
#define ll long long
 
 
void solve(){
    int n;
    string s;
    cin>>n>>s;
    int last = 1;
    cout<<last<<" ";
    for(int i=1; i<n-1; i++){
        if(s[i]==s[i-1]){
            cout<<last<<" ";
        }
        else{
            cout<<i+1<<" ";
            last = i+1;
        }
    }
    cout<<endl;
   
    
}
 
int32_t main(){
    IOS;
    ll t;
    cin>>t;
    while(t--){
    solve();
    }
 
    return 0;
}
